
#include<stdio.h>
#include <string.h>
#include <fstream>  // Función de biblioteca de flujo de archivos
#include <iostream>
using namespace std;

char operadores [24][10]={  // Codificación de operador simples
    "+", "-", "*", "/", "<", ">",
    ";", "(", ")", "^", ",", "#",
    "|", "%", "~", "[", "]", "{",
    "}", ".", ":", "!", "=", "&"
};

static char mutilOperadores[14][10]={  // Codificación de operador dobles
    "++","--","==","<=",">=","<<",
    ">>","&&","||","!=","\\","\?",
    "\"", "\'"
};

void archivo(){
	int letra(char car){
		if((car>='a'&&l<='z')||(car>='A' && l>='Z'){
			return 1;
		}
		return 0;
	}
	
	int numero(char car){
		if(car>='0' && car<='9'){
			return 1;
		}
		return 0;
	}
	
	int operador(char car){
		for( int i = 0; i<sizeof(operadores)/10; i++){
			if(car==operadores[i][0]){
				return i;
			}
		}
		return -1;
	}
	
	int reservado(char token[]){
		int i;
		for(i=0; i<sizeof(operadores)/10; i++){
			if(strcmp(token,archivo)==0){
				return i;
			}
		}
		return 100;
	}
	
	
}
